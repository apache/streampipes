    "karma-verbose-reporter": "0.0.8",
    "lint-staged": "^15.2.0",
    "mustache": "^4.2.0",
    "ng-lint-staged": "^12.0.4",
    "ng-packagr": "^18.2.1",
    "path": "^0.12.7",
    "prettier": "3.2.5",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "5.4.5"
  }
}
