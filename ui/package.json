    "@ngx-loading-bar/http-client": "6.0.2",
    "@panzoom/panzoom": "^4.5.1",
    "@swimlane/ngx-charts": "^20.5.0",
    "angular-gridster2": "^18.0.1",
    "codemirror": "^5.65.11",
